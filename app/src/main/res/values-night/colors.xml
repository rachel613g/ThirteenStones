<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- For colored text; default is black/dark-gray (Day) or white/light-gray (Night) -->
    <color name="primaryTextColoredColor">@color/primaryLightColor</color>
    <color name="secondaryTextColoredColor">@color/primaryColor</color>

    <!-- Matching colored background -->
    <color name="backgroundColoredColor">@color/primaryDarkColor</color>

    <!--Used for Splash screen background, and elsewhere as needed-->
    <color name="defaultBackgroundColor">@android:color/background_dark</color>
</resources>